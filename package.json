{
  "name": "vue-visjs-timeline",
  "description": "Vue component for the vis.js timeline module.",
  "version": "1.1.0",
  "private": false,
  "scripts": {
    "lint": "vue-cli-service lint",
    "build": "vue-cli-service build --target lib --name vue-visjs-timeline ./src/timeline.vue"
  },
  "main": "./dist/vue-visjs-timeline.common.js",
  "files": [
    "dist/*",
    "*.js",
    "*.css",
    "*.map"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/shri-bhat/vue-visjs-timeline.git"
  },
  "author": "Shripad Bhat <shripad.canada@gmail.com>",
  "license": "MIT",
  "keywords": [
    "vue",
    "vis",
    "visjs"
  ],
  "dependencies": {
    "core-js": "^2.6.5",
    "npm": "^6.9.0",
    "vis": "^4.21.0",
    "vue": "^2.6.9"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
